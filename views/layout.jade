doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(href='https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,600,600i,700,800', rel='stylesheet')
    link(rel='stylesheet', type='text/css', href='/css/bootstrap.min.css')
    link(rel='stylesheet', type='text/css', href='/css/style.css')
    link(rel='stylesheet', type='text/css', href='/css/responsive.css')
    link(rel='stylesheet', type='text/css', href='/css/font-awesome.min.css')
    link(rel='shortcut icon', href='/images/favicon.ico')
    //if lt IE 9
      script(src='//html5shiv.googlecode.com/svn/trunk/html5.js')
    //if lt IE 9
      script(src='//html5shiv.googlecode.com/svn/trunk/html5shiv-printshiv.js.js')
    script(src='/js/jquery.min.js', type='text/javascript')
    script.
    script(src='/js/common.js')
    script(src='/js/main.js')
    script(type='text/javascript').
      $(document).ready(function(){
      $(".menu-icon").click(function(){
      if($("body").hasClass("body-nav")){
      $( "body" ).removeClass( "body-nav" );
      } else {
      $( "body" ).addClass("body-nav");
      }
      });
      });
    title #{title}
  body
    include partials/header
    .container.main-middle
      include partials/left_menu
      block content
    include partials/footer
